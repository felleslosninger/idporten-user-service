<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
        http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://repo1.maven.org/maven2</url>
                </repository>
                <repository>
                    <id>github-oidc-sdk</id>
                    <url>https://maven.pkg.github.com/felleslosninger/.github</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <mirrors>
        <mirror>
            <id>difi-artifactory-http-unblock</id>
            <mirrorOf>difi-artifactory</mirrorOf>
            <name>Dummy mirror to override default blocking mirror that blocks http</name>
            <url>http://eid-artifactory.dmz.local:8080/artifactory/libs-release</url>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>github-oidc-sdk</id>
            <username>${env.GIT_PACKAGE_USERNAME}</username>
            <password>${env.GIT_PACKAGE_TOKEN}</password>
        </server>
        <server>
            <id>github</id>
            <username>${env.GIT_PACKAGE_USERNAME}</username>
            <password>${env.GIT_PACKAGE_TOKEN}</password>
        </server>
    </servers>
</settings>